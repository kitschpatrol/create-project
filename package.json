{
	"name": "@kitschpatrol/create-project",
	"version": "1.2.1",
	"description": "Kitschpatrol's TypeScript project templates.",
	"keywords": [
		"cli",
		"template",
		"bingo"
	],
	"homepage": "https://github.com/kitschpatrol/create-project",
	"bugs": "https://github.com/kitschpatrol/create-project/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/create-project.git"
	},
	"license": "MIT",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"type": "module",
	"bin": "dist/index.js",
	"files": [
		"dist",
		"templates",
		"templates/**/.gitignore",
		"templates/**/.npmrc"
	],
	"scripts": {
		"build": "tsdown --no-fixed-extension",
		"clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
		"fix": "ksc fix",
		"lint": "ksc-repo lint ; mdat readme check readme.md ; ksc-typescript lint ; ksc-eslint lint ; ksc-stylelint lint ; ksc-cspell lint ; ksc-knip lint ; ksc-prettier lint",
		"release": "bumpp --commit 'Release: %s' && pnpm run build && NPM_AUTH_TOKEN=$(op read 'op://Personal/npm/token') && pnpm publish",
		"test": "vitest run",
		"update-templates": "for dir in ./templates/*; do taze -w --cwd \"$dir\"; done"
	},
	"dependencies": {
		"bingo": "^0.9.2",
		"bingo-fs": "^0.5.6",
		"bingo-handlebars": "^0.1.0"
	},
	"devDependencies": {
		"@kitschpatrol/shared-config": "^5.10.0",
		"@types/node": "~20.19.27",
		"bumpp": "^10.3.2",
		"mdat": "^1.3.3",
		"taze": "^19.9.2",
		"tsdown": "^0.18.3",
		"typescript": "~5.9.3",
		"vitest": "^4.0.16",
		"zod": "^3.25.76"
	},
	"packageManager": "pnpm@10.26.2",
	"engines": {
		"node": ">=20.19.0"
	},
	"publishConfig": {
		"access": "public"
	}
}

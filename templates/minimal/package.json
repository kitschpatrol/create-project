{
	"name": "{{{github-repository}}}",
	"version": "0.0.0",
	"private": true,
	"description": "A {{{type}}} project.",
	"keywords": [
		"{{{type}}}"
	],
	"homepage": "https://github.com/{{{github-owner}}}/{{{github-repository}}}",
	"bugs": "https://github.com/{{{github-owner}}}/{{{github-repository}}}/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/{{{github-owner}}}/{{{github-repository}}}.git"
	},
	"license": "MIT",
	"author": {
		"name": "{{{author-name}}}",
		"email": "{{{author-email}}}",
		"url": "{{{author-url}}}"
	},
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "tsx script.ts",
		"clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
		"dev": "tsx --watch script.ts",
		"fix": "ksc fix",
		"lint": "ksc lint",
		"release": "bumpp --commit 'Release: %s'",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@types/node": "~20.19.35"
	},
	"devDependencies": {
		"@kitschpatrol/shared-config": "^6.0.1",
		"bumpp": "^10.4.1",
		"tsx": "^4.21.0",
		"typescript": "~5.9.3"
	},
	"packageManager": "pnpm@10.30.3",
	"engines": {
		"node": ">=20.19.0"
	}
}

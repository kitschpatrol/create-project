{
	"name": "{{{github-repository}}}",
	"version": "0.0.0",
	"description": "A {{{type}}} project.",
	"keywords": [
		"cli",
		"npm-package"
	],
	"homepage": "https://github.com/{{{github-owner}}}/{{{github-repository}}}",
	"bugs": "https://github.com/{{{github-owner}}}/{{{github-repository}}}/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/{{{github-owner}}}/{{{github-repository}}}.git"
	},
	"license": "MIT",
	"author": {
		"name": "{{{author-name}}}",
		"email": "{{{author-email}}}",
		"url": "{{{author-url}}}"
	},
	"type": "module",
	"bin": {
		"{{{cli-command-name}}}": "dist/index.js"
	},
	"files": [
		"dist/*"
	],
	"scripts": {
		"build": "tsdown --no-fixed-extension --dts false",
		"clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
		"fix": "ksc fix",
		"lint": "ksc lint",
		"release": "bumpp --commit 'Release: %s' && pnpm run build && NPM_AUTH_TOKEN=$({{{npm-auth-command}}}) && pnpm publish",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@types/node": "~20.19.30",
		"@types/yargs": "^17.0.35",
		"lognow": "^0.3.2",
		"yargs": "^18.0.0"
	},
	"devDependencies": {
		"@kitschpatrol/shared-config": "^5.13.0",
		"bumpp": "^10.4.0",
		"mdat-plugin-cli-help": "^1.0.5",
		"tsdown": "^0.20.1",
		"typescript": "~5.9.3"
	},
	"packageManager": "pnpm@10.28.1",
	"engines": {
		"node": ">=20.19.0"
	}
}
